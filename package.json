{
  "name": "url-relation",
  "description": "Determine the relation between two URLs.",
  "version": "1.0.0-alpha11",
  "license": "MIT",
  "author": "Steven Vachon <contact@svachon.com> (https://www.svachon.com/)",
  "browser": {
    "parse-domain": "./shims/parse-domain"
  },
  "repository": "stevenvachon/url-relation",
  "dependencies": {
    "any-match": "^1.0.1",
    "deep-freeze-node": "^1.1.2",
    "defined": "^1.0.0",
    "evaluate-value": "^1.0.2",
    "isurl": "^1.0.0",
    "parse-domain": "^1.1.0",
    "strip-www": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "coveralls": "^3.0.0",
    "gzip-size-cli": "^2.0.0",
    "incomplete-url": "^1.0.2",
    "mocha": "^3.4.2",
    "npm-watch": "~0.2.0",
    "nyc": "^11.0.3",
    "uglify-js": "^3.0.23",
    "universal-url": "^1.0.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "ci": "npm run test && nyc report --reporter=text-lcov | coveralls",
    "posttest": "nyc report --reporter=html && browserify index.js --global-transform [ babelify --presets [ es2015 ] ] --standalone urlRelation | uglifyjs --compress --mangle | gzip-size",
    "test": "nyc --reporter=text-summary mocha test/ --check-leaks --bail",
    "watch": "npm-watch test"
  },
  "watch": {
    "test": {
      "inherit": true,
      "patterns": [
        "index.js",
        "test/**/*.{js,json}"
      ]
    }
  },
  "files": [
    "index.js",
    "shims/parse-domain.js"
  ],
  "keywords": [
    "uri",
    "url",
    "relation",
    "whatwg"
  ]
}
