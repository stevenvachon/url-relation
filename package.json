{
  "name": "url-relation",
  "description": "Determine the relation between two URLs.",
  "version": "1.0.0-alpha9",
  "license": "MIT",
  "author": "Steven Vachon <contact@svachon.com> (https://www.svachon.com/)",
  "main": "lib/url-relation",
  "browser": {
    "parse-domain": "./lib/parse-domain"
  },
  "repository": "stevenvachon/url-relation",
  "dependencies": {
    "any-match": "^1.0.1",
    "deep-freeze-node": "^1.1.2",
    "defined": "^1.0.0",
    "evaluate-value": "^1.0.2",
    "isurl": "^1.0.0-alpha5",
    "parse-domain": "^1.1.0",
    "strip-www": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.1",
    "coveralls": "^2.13.1",
    "gzip-size-cli": "^2.0.0",
    "mocha": "^3.4.2",
    "npm-watch": "~0.2.0",
    "nyc": "^11.0.2",
    "uglify-js": "^3.0.15",
    "universal-url": "^1.0.0-alpha"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "ci": "npm run test && nyc report --reporter=text-lcov | coveralls",
    "posttest": "nyc report --reporter=html && browserify lib/url-relation --global-transform [ babelify --presets [ es2015 ] ] --standalone urlRelation | uglifyjs --compress --mangle | gzip-size",
    "pretest": "babel src --out-dir=lib --presets=es2015 --source-maps=true",
    "test": "nyc --reporter=text-summary mocha test/ --reporter=spec --check-leaks --bail",
    "watch": "npm-watch test"
  },
  "watch": {
    "test": {
      "inherit": true,
      "patterns": "{src,test}/**/*.{js,json}"
    }
  },
  "files": [
    "lib/parse-domain.js",
    "lib/url-relation.js"
  ],
  "keywords": [
    "uri",
    "url",
    "relation",
    "whatwg"
  ]
}
